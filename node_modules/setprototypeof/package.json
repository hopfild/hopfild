  "keywords": [
    "polyfill",
    "object",
    "setprototypeof"
  ],
  "homepage": "https://github.com/wesleytodd/setprototypeof",
  "bugs": {
    "url": "https://github.com/wesleytodd/setprototypeof/issues"
  },
  "repository": {
    "type": "git",
