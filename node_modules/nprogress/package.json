{
  "name": "nprogress",
  "version": "0.2.0",
  "description": "Simple slim progress bars",
  "repository": {
    "type": "git",
    "url": "https://github.com/rstacruz/nprogress.git"
  },
  "license": "MIT",
  "author": "Rico Sta. Cruz <hi@ricostacruz.com>",
  "main": "nprogress.js",
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "~1.6.1",
    "jquery": "^2.1.1",
    "jsdom": "^5.4.1",
    "mocha": "^2.2.4",
    "mocha-jsdom": "^0.3.0"
  },
  "jspm": {
    "format": "global",
    "shim": {
      "nprogress": {
        "deps": [
          "./nprogress.css!"
        ]
      }
    },
    "dependencies": {
      "css": "*"
    }
  },
  "spm": {
    "main": "nprogress.js",
    "output": [
      "nprogress.css"
    ],
    "ignore": [
      "support",
      "test",
      "vendor"
    ]
  }
}
