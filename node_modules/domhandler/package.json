  "description": "handler for htmlparser2 that turns pages into a dom",
  "keywords": [
    "dom",
    "htmlparser2"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/fb55/DomHandler.git"
  },
  "license": "BSD-2-Clause",
