{
  "name": "section-matter",
  "version": "1.0.0",
  "description": "Like front-matter, but supports multiple sections in a document.",
  "keywords": [
    "matter",
    "section"
  ],
  "homepage": "https://github.com/jonschlinkert/section-matter",
  "bugs": {
    "url": "https://github.com/jonschlinkert/section-matter/issues"
  },
  "repository": "jonschlinkert/section-matter",
  "license": "MIT",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "kind-of": "^6.0.0"
  },
  "devDependencies": {
    "gulp-format-md": "^1.0.0",
    "js-yaml": "^3.10.0",
    "mocha": "^4.0.1"
  },
  "engines": {
    "node": ">=4"
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "gray-matter",
        "assemble",
        "verb"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
