{
  "name": "pretty-time",
  "version": "1.1.0",
  "description": "Easily format the time from node.js `process.hrtime`. Works with timescales ranging from weeks to nanoseconds.",
  "keywords": [
    "convert",
    "date",
    "format",
    "formatting",
    "hour",
    "hrtime",
    "micro",
    "milli",
    "minute",
    "nano",
    "nanosecond",
    "pretty",
    "second",
    "time",
    "week"
  ],
  "homepage": "https://github.com/jonschlinkert/pretty-time",
  "bugs": {
    "url": "https://github.com/jonschlinkert/pretty-time/issues"
  },
  "repository": "jonschlinkert/pretty-time",
  "license": "MIT",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "main": "index.js",
  "files": [
    "index.js",
    "utils.js"
  ],
  "scripts": {
    "test": "mocha"
  },
  "devDependencies": {
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.5.3"
  },
  "engines": {
    "node": ">=4"
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "o-clock",
        "seconds",
        "time-stamp",
        "timescale",
        "week",
        "weekday",
        "year"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
