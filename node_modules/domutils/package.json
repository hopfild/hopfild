{
  "name": "domutils",
  "version": "1.7.0",
  "description": "utilities for working with htmlparser2's dom",
  "keywords": [
    "dom",
    "htmlparser2"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/FB55/domutils.git"
  },
  "license": "BSD-2-Clause",
  "author": "Felix Boehm <me@feedic.com>",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "mocha test/tests/**.js && jshint index.js test/**/*.js lib/*.js"
  },
  "dependencies": {
    "dom-serializer": "0",
    "domelementtype": "1"
  },
  "devDependencies": {
    "domhandler": "2",
    "htmlparser2": "~3.9.2",
    "jshint": "~2.9.4",
    "mocha": "~3.2.0"
  },
  "jshintConfig": {
    "proto": true,
    "unused": true,
    "eqnull": true,
    "undef": true,
    "quotmark": "double",
    "eqeqeq": true,
    "trailing": true,
    "node": true,
    "globals": {
      "describe": true,
      "it": true,
      "beforeEach": true
    }
  }
}
