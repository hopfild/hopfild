{\n          this.slotProps.mail = '';\n          if (popupEnabled) event.$emit('submited', { result: 'error' });\n        })\n        .then(res => {\n          this.slotProps.mail = '';\n          if (popupEnabled) event.$emit('submited', res);\n        });\n    },\n  },\n};\n"]}]}