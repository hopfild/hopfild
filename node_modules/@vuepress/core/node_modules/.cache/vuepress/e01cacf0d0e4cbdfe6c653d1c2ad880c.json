  },\n    author: {\n      type: String,\n    },\n    date: {\n      type: String,\n    },\n    location: {\n      type: String,\n    },\n  },\n  computed: {\n    resolvedDate() {\n      return dayjs(this.date).format(\n        this.$themeConfig.dateFormat || 'ddd, MMM DD YYYY'\n      )\n    },\n    resolvedTags() {\n      if (!this.tags || Array.isArray(this.tags)) return this.tags\n      return [this.tags]\n    },\n  },\n}\n</script>\n"]}]}